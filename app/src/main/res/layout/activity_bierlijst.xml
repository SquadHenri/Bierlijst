<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Bierlijst">

    <TextView
        android:id="@+id/titleBierlijst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Bier!"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/B4sbkrijgen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/IG_B4"
        app:layout_constraintTop_toBottomOf="@+id/addB4" />

    <TextView
        android:id="@+id/B2sbgeven"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/IG_B2"
        app:layout_constraintTop_toBottomOf="@+id/substractB2" />

    <ImageButton
        android:id="@+id/substractB5"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="substractsteven"
        app:layout_constraintStart_toStartOf="@+id/IG_B5"
        app:layout_constraintTop_toBottomOf="@+id/IG_B5"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        app:tint="#000000" />

    <ImageButton
        android:id="@+id/substractB2"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="substractSven"
        app:layout_constraintStart_toStartOf="@+id/IG_B2"
        app:layout_constraintTop_toBottomOf="@+id/IG_B2"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        app:tint="#000000" />

    <ImageButton
        android:id="@+id/IG_B5"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="16dp"
        android:longClickable="true"
        android:onClick="stevenBier"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/IG_B2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/IG_B2"
        app:layout_constraintTop_toBottomOf="@+id/editTextB2"
        app:layout_constraintVertical_bias="0.208"
        app:srcCompat="@drawable/ic_vg" />

    <TextView
        android:id="@+id/B5sbgeven"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/IG_B5"
        app:layout_constraintTop_toBottomOf="@+id/substractB5" />

    <TextView
        android:id="@+id/B3sbkrijgen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/IG_B3"
        app:layout_constraintTop_toBottomOf="@+id/addB3" />

    <TextView
        android:id="@+id/B5sbkrijgen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/IG_B5"
        app:layout_constraintTop_toBottomOf="@+id/addB5" />

    <ImageButton
        android:id="@+id/addB1"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="addThijs"
        app:layout_constraintEnd_toEndOf="@+id/IG_B1"
        app:layout_constraintTop_toTopOf="@+id/substractB1"
        app:srcCompat="@android:drawable/ic_input_add"
        app:tint="#000000" />

    <ImageButton
        android:id="@+id/addB2"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="addSven"
        app:layout_constraintEnd_toEndOf="@+id/IG_B2"
        app:layout_constraintTop_toBottomOf="@+id/IG_B2"
        app:srcCompat="@android:drawable/ic_input_add"
        app:tint="#000000" />

    <ImageButton
        android:id="@+id/IG_B1"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:longClickable="true"
        android:onClick="thijsBier"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/IG_B2"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/IG_B2"
        app:srcCompat="@drawable/ic_thijs" />

    <TextView
        android:id="@+id/B4sbgeven"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/IG_B4"
        app:layout_constraintTop_toBottomOf="@+id/substractB4" />

    <ImageButton
        android:id="@+id/addB4"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="addRowin"
        app:layout_constraintEnd_toEndOf="@+id/IG_B4"
        app:layout_constraintTop_toTopOf="@+id/editTextB4"
        app:srcCompat="@android:drawable/ic_input_add"
        app:tint="#000000" />

    <EditText
        android:id="@+id/editTextB3"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="numberSigned"
        app:layout_constraintEnd_toStartOf="@+id/addB3"
        app:layout_constraintStart_toEndOf="@+id/substractB3"
        app:layout_constraintTop_toTopOf="@+id/substractB3" />

    <ImageButton
        android:id="@+id/substractB3"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="substractEtienne"
        app:layout_constraintStart_toStartOf="@+id/IG_B3"
        app:layout_constraintTop_toBottomOf="@+id/IG_B3"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        app:tint="#000000" />

    <TextView
        android:id="@+id/B3sbgeven"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/IG_B3"
        app:layout_constraintTop_toBottomOf="@+id/substractB3" />

    <ImageButton
        android:id="@+id/addB3"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="addEtienne"
        app:layout_constraintEnd_toEndOf="@+id/IG_B3"
        app:layout_constraintTop_toBottomOf="@+id/IG_B3"
        app:srcCompat="@android:drawable/ic_input_add"
        app:tint="#000000" />

    <ImageButton
        android:id="@+id/IG_B2"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="24dp"
        android:longClickable="true"
        android:onClick="svenBier"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleBierlijst"
        app:srcCompat="@drawable/ic_sven" />

    <EditText
        android:id="@+id/editTextB5"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="numberSigned"
        app:layout_constraintEnd_toStartOf="@+id/addB5"
        app:layout_constraintStart_toEndOf="@+id/substractB5"
        app:layout_constraintTop_toBottomOf="@+id/IG_B5" />

    <TextView
        android:id="@+id/B1sbgeven"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/IG_B1"
        app:layout_constraintTop_toBottomOf="@+id/substractB1" />

    <TextView
        android:id="@+id/B2sbkrijgen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/IG_B2"
        app:layout_constraintTop_toBottomOf="@+id/addB2" />

    <EditText
        android:id="@+id/editTextB2"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="numberSigned"
        app:layout_constraintEnd_toStartOf="@+id/addB2"
        app:layout_constraintStart_toEndOf="@+id/substractB2"
        app:layout_constraintTop_toBottomOf="@+id/IG_B2" />

    <EditText
        android:id="@+id/editTextB1"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="numberSigned"
        app:layout_constraintEnd_toStartOf="@+id/addB1"
        app:layout_constraintStart_toEndOf="@+id/substractB1"
        app:layout_constraintTop_toTopOf="@+id/substractB1"
        tools:text="0" />

    <ImageButton
        android:id="@+id/addB5"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="addsteven"
        app:layout_constraintEnd_toEndOf="@+id/IG_B5"
        app:layout_constraintTop_toBottomOf="@+id/IG_B5"
        app:srcCompat="@android:drawable/ic_input_add"
        app:tint="#000000" />

    <EditText
        android:id="@+id/editTextB4"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="numberSigned"
        app:layout_constraintEnd_toStartOf="@+id/addB4"
        app:layout_constraintStart_toEndOf="@+id/substractB4"
        app:layout_constraintTop_toBottomOf="@+id/IG_B4"
        tools:text="50" />

    <ImageButton
        android:id="@+id/IG_B4"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:longClickable="true"
        android:onClick="rowinBier"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="@+id/IG_B1"
        app:layout_constraintStart_toStartOf="@+id/IG_B1"
        app:layout_constraintTop_toTopOf="@+id/IG_B5"
        app:srcCompat="@drawable/ic_rowin" />

    <TextView
        android:id="@+id/B1sbkrijgen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="50"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/IG_B1"
        app:layout_constraintTop_toBottomOf="@+id/addB1" />

    <ImageButton
        android:id="@+id/substractB4"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="substractRowin"
        app:layout_constraintStart_toStartOf="@+id/IG_B4"
        app:layout_constraintTop_toTopOf="@+id/editTextB4"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        app:tint="#000000" />

    <ImageButton
        android:id="@+id/substractB1"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:onClick="substractThijs"
        app:layout_constraintStart_toStartOf="@+id/IG_B1"
        app:layout_constraintTop_toBottomOf="@+id/IG_B1"
        app:srcCompat="@android:drawable/button_onoff_indicator_on"
        app:tint="#000000" />

    <ImageButton
        android:id="@+id/IG_nonMM"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="@+id/IG_B3"
        app:layout_constraintStart_toStartOf="@+id/IG_B3"
        app:layout_constraintTop_toTopOf="@+id/IG_B5"
        app:srcCompat="@drawable/beer512"
        app:tint="#000000" />

    <TextView
        android:id="@+id/NietMMers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voor niet MMers"
        android:textAlignment="center"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/IG_nonMM"
        app:layout_constraintStart_toStartOf="@+id/IG_nonMM"
        app:layout_constraintTop_toBottomOf="@+id/IG_nonMM" />

    <ImageButton
        android:id="@+id/IG_B3"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="8dp"
        android:longClickable="true"
        android:onClick="etienneBier"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/IG_B2"
        app:layout_constraintTop_toTopOf="@+id/IG_B2"
        app:srcCompat="@drawable/ic_etienne" />

</androidx.constraintlayout.widget.ConstraintLayout>